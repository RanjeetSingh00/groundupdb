    "version": "0.2.0",
    "configurations": [
        {
            "name": "(Windows) Launch Tests",
            "type": "cppvsdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/groundupdb-tests/Debug/groundupdb-tests.exe",
            "args": [" datatypes-customtypes-memory"],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}/build/groundupdb-tests/Debug",
            "environment": [],
            "externalConsole": false,
            "logging": {
              "moduleLoad": false,
              "trace": true
            }
        },
        
        {
            "name": "(Windows) Performance Tests",
            "type":"cppvsdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/groundupdb-tests/Debug/groundupdb-tests.exe",
            "args": ["profiling-100k"],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}/build/groundupdb-tests/Debug",
            "environment": [],
            "externalConsole": false,
            "logging": {
              "moduleLoad": false,
              "trace": false
            }
        }
    ]
}